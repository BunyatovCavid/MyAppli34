<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="5dp">

    <ImageView
        android:id="@+id/goodsimage"
        android:layout_width="200dp"
        android:layout_height="300dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginRight="0dp"/>

    <TextView
        android:id="@+id/goodstext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        android:text="kljsfljslfjsl"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/goodsimage"
        tools:layout_editor_absoluteX="40dp"
        android:layout_marginTop="15dp"
        />

    <ImageView
        android:id="@+id/goodsiconone"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints"
        android:src="@drawable/baseline_shopping_cart_24"
        app:layout_constraintTop_toBottomOf="@id/goodstext"
        />

    <TextView
        android:id="@+id/goodsicontextone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        android:text="8665"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/goodsiconone"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/goodstext"
        />


    <ImageView
        android:id="@+id/goodsicontwo"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/baseline_remove_shopping_cart_24"
        app:layout_constraintEnd_toStartOf="@id/goodsicontexttwo"
        app:layout_constraintTop_toBottomOf="@id/goodstext"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/goodsicontexttwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"
        android:text="8900"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/goodstext"
        />





</androidx.constraintlayout.widget.ConstraintLayout>